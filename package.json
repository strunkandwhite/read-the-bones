{
  "name": "read-the-bones",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "tsx scripts/sync-sheets.ts && tsx scripts/generate-draft-data.ts",
    "dev": "next dev",
    "prebuild": "tsx scripts/generate-draft-data.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"scripts/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"scripts/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "screenshot": "npx tsx scripts/screenshot.ts",
    "sync-sheets": "tsx scripts/sync-sheets.ts",
    "db:migrate": "tsx src/core/db/migrate.ts",
    "ingest": "tsx src/core/db/ingest.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@tanstack/react-table": "^8.21.3",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^5.0.2",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "next": "16.1.1",
    "openai": "^4.77.0",
    "papaparse": "^5.5.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "memfs": "^4.56.2",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
